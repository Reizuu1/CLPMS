<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".lessee.LesseeDashboard">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/container"
        android:layout_above="@+id/bottomNavigationViewLessee"/>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="108dp"
        android:id="@+id/bottomNavigationViewLessee"
        app:itemIconTint="@color/white"
        app:itemTextColor="@color/white"
        app:itemBackground="@drawable/blue_line"
        app:labelVisibilityMode="unlabeled"
        android:padding="10dp"
        android:layout_alignParentBottom="true"
        app:menu="@menu/lessee_bottom_navigation"
        android:background="@drawable/bottomnavbg"/>


</RelativeLayout>